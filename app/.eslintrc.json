{
    "env": {
        "es2021": true,
        "node": true,
        "jest/globals": true
    },
    "extends": [
        "airbnb-base",
        "plugin:jest/recommended"
    ],
    "plugins": ["jest"],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-console": 2,
        "import/no-unresolved": 0,
        "camelcase": 0
    },
    "settings": {
        "jest": {
            "version": "^26.6.3"
        }
    }
}
